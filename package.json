{
  "name": "remix-cloudflare-wakatake",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "migrations:gen": "drizzle-kit generate:sqlite",
    "local:migrations:apply": "wrangler d1 migrations apply artora --local",
    "migrations:apply": "NO_D1_WARNING=true wrangler d1 migrations apply artora"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.6.0",
    "@remix-run/cloudflare-pages": "^2.6.0",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "drizzle-orm": "^0.29.4",
    "embla-carousel-react": "^8.0.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-google": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.6.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "daisyui": "^4.7.2",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.0",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
